{
  "buildCommand": "npm run build:client && npm run build:api",
  "outputDirectory": "dist/public",
  "functions": {
    "api/**/*.ts": {
    "runtime": "@vercel/node@20.6.1"

    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
